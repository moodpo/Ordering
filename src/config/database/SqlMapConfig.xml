<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<!-- 使用命名空间 -->
	<settings useStatementNamespaces="true" />
	
	<!-- User -->
	<sqlMap resource="config/database/oracle/user.xml"/>
	<!-- Guser -->
	<sqlMap resource="config/database/oracle/guser.xml"/>
	<!-- Group -->
	<sqlMap resource="config/database/oracle/group.xml"/>
	<!-- Money -->
	<sqlMap resource="config/database/oracle/money.xml"/>
	<!-- Dic -->
	<sqlMap resource="config/database/oracle/dic.xml"/>
	<!-- Price -->
	<sqlMap resource="config/database/oracle/price.xml"/>
	<!-- Param -->
	<sqlMap resource="config/database/oracle/param.xml"/>
	
</sqlMapConfig>


